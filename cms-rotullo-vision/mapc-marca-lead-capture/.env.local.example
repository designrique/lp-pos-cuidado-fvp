# Configurações do Mautic
VITE_MAUTIC_BASE_URL=https://crm.mapc.com.br
VITE_MAUTIC_USERNAME=api_username
VITE_MAUTIC_PASSWORD=api_password
VITE_MAUTIC_SITE_LEADS_LIST=1
VITE_MAUTIC_POTENTIAL_CLIENTS_LIST=2

# Credenciais OAuth 2.0 para o Mautic
VITE_MAUTIC_OAUTH_CLIENT_ID=client_id_aqui
VITE_MAUTIC_OAUTH_CLIENT_SECRET=client_secret_aqui
VITE_MAUTIC_REDIRECT_URI=https://crm.mapc.com.br/oauth/callback

# Observação: Substitua api_username, api_password, client_id_aqui e client_secret_aqui
# pelas credenciais reais do Mautic. As credenciais OAuth são mais seguras e recomendadas.
